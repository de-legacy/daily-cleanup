<%- include('partials/header', {title: 'Worker list'})  %>

<main class="content">
	<div class="row">
		<div class="col-md-12">
			<h3 class="no-mt">All Workers</h3><!-- /.no-mt -->
			<table class="table">
				<thead>
					<tr>
						<th>Nama Worker</th>
						<th>No. Telepon</th>
						<th>Email</th>
						<th>Rating</th>
						<th>Action</th>
					</tr>
				</thead>
				<% allWorker.forEach(worker => {%>
					<% if (worker.averagerating >=3 ) {%>
				<tbody>
					<tr>
						<td><%= worker.fullname %></td>
						<td><%= worker.phone %></td>
						<td><%= worker.email %></td>
						<td>

							<div class="rating-star">
							<%- Helper.getStars(worker.averagerating); %>
							</div>
						</td>
						<td><a href="/user/order-services/<%= worker.id %>">Pesan</a></td>
					</tr>
				</tbody>
					<% } %>
				<%})%>
			</table>

		</div><!-- /.col-md-12 -->
	</div><!-- /.row -->
</main><!-- /.content -->

<%- include('partials/footer') %>