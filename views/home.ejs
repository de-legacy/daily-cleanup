<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<% allUser.forEach(user => {%>
		<table>
			<tr>
				<td><%= user.fullname %></td>
				<td><%= user.address %></td>
				<td><%= user.phone %></td>
				<td><%= user.email %></td>
				<td><%= user.username %></td>
				<td><%= user.password %></td>
				<td><%= user.privelege %></td>
				<td><a href="/user/edit/<%= user.id %> "> edit </a></td>
				<td><a href="/user/delete/<%= user.id %> "> delete </a></td>
				<td></td>
			</tr>	
		</table>
	<%})%>
</body>
</html>